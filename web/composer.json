{
    "name": "tomchampion/tomchampionxyz",
    "description": "Slim/Vue website for tomchampion.xyz",
    "autoload": {
        "psr-4": {
            "App\\": "app"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "require": {
        "slim/slim": "4.*",
        "slim/twig-view": "^3.0.0",
        "twig/extensions": "*",
        "slim/csrf": "^1.0.0",
        "league/container": "^3.3",
        "slim/psr7": "^1.1",
        "vlucas/phpdotenv": "^4.1",
        "ext-json": "*",
        "illuminate/database": "^7.14",
        "vlucas/valitron": "^1.4",
        "unsplash/unsplash": "^2.5",
        "monolog/monolog": "^2.1",
        "phpunit/phpunit": "^9.2",
        "fzaninotto/faker": "^1.9",
        "ramsey/uuid": "^4.0",
        "illuminate/events": "^7.16",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0",
        "phpro/grumphp": "^0.19.0"
    }
}
